@model ProjConcept.Models.UserNote

@{
    ViewBag.Title = "Delete Note";
}

<h3>Are you sure you want to delete this Note?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NoteTitle)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NoteTitle)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Note)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Note)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.NoteLastUpdate)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NoteLastUpdate)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-warning" />
            @Html.ActionLink("Cancel", "Index", "UserNotes", null, new { @class = "btn btn-primary" })
        </div>
    }
</div>
